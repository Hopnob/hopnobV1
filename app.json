{
  "expo": {
    "name": "Hopnob",
    "slug": "hopnob",
    "version": "2.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/launchPage.png",
      "resizeMode": "strech",
      "backgroundColor": "#D9D9D9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "testing.hopnobapp"
    },
    "android": {
      "package": "testing.hopnobapp",
      "versionCode":8,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }

    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f7ecb39c-46e6-4168-b551-d9488402901d"
      }
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
